{
  "manifest_version": 2,
  "name": "Shared Host",
  "version": "0.0.2",
  "icons": { 
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_title": "Inactive.",
    "default_icon": "off.svg",
    "default_popup": "options.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.mlsmatrix.com/Matrix/*"],
      "js": ["content.js"],
      "css": ["vue-index.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "background",
    "*://*/*"
  ]
}
